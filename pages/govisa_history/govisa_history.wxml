<view style="width:100%;height:{{height}}px;background-color:#1380e2;display: inline-block;">
	<view class="border_wai" style="position:fixed;background-color:#1380e2;z-index:1000;margin:0 auto;">
		<view class="input_css">
			<rui-dtpicker start="2020-01-01" end="2035-12-31" value="2020-01-01" fields="day"
				bindchangedatepicker="changeDate" bindcanceldatepicker="cancelDate"></rui-dtpicker>
			<text class="cuIcon-unfold lg text-gray"></text>
		</view>
		<view class="jianhao">-</view>
		<view class="input_css">
			<rui-dtpicker2 wx:if="{{enddate}}" start="2020-01-01" end="2035-12-31" value="{{enddate}}" fields="day"
				bindchangedatepicker="changeDate" bindcanceldatepicker="cancelDate"></rui-dtpicker2>
			<text class="cuIcon-unfold lg text-gray" style="clear:left;"></text>
		</view>
		<view class="search1" bindtap="search">
			<view class="cuIcon-search lg text-blue" style="margin-left:10px;"></view>
		</view>
	</view>
	<!-- 列表元素 -->
	<view style="margin-top: 10px;background-color:#1380e2;">
		<view style='height:20px;'></view>
		<!-- 人员入厂申清 -->
		<view wx:for="{{applylist}}">
			<view class='item_content' wx:if="{{item.busType==0}}">
				<view class='item_content_title'>
					<label class='item_content_title_left'>人员入厂申请</label><button wx:if="{{item.statusName=='审批通过'}}"
						class="cu-btn round line-white item_content_right_button" data-bustype="{{item.busType}}"
						data-objuid="{{item.objuid}}" bindtap="qrcode">二维码</button>
				</view>
				<view style='position: relative;'>
					<view class='item_info'>
						<view class='item_info_left'>
							<view class='info_item'>
								<view class='info_intem_label'>访客姓名:</view><label>{{item.name}}</label>
							</view>
							<view class='info_item'>
								<view class='info_intem_label'>职务:</view><label>{{item.businessName}}</label>
							</view>
							<view class='info_item'>
								<view class='info_intem_label'>手机号:</view><label>{{item.phoneNumber}}</label>
							</view>
							<view class='info_item'>
								<view class='info_intem_label'>来方事由:</view><label>{{item.resonForVisiting}}</label>
							</view>
							<view class='info_item'>
								<view class='info_intem_label'>接待人:</view><label>{{item.receptionist}}</label>
							</view>
							<view class='info_item'>
								<view class='info_intem_label'>授权时间:</view><label>{{item.personStartDate}}</label><label>起</label>
								<view class='info_intem_label'></view><label>{{item.personEndDate}}</label><label>止</label>
							</view>
							<view wx:if="{{item.plateNumber}}" class='info_item'>
								<view class='info_intem_label'>车牌号:</view><label>{{item.plateNumber}}</label>
							</view>
							<view wx:if="{{item.carTypeName}}" class='info_item'>
								<view class='info_intem_label'>车类型:</view><label>{{item.carTypeName}}</label>
							</view>
							<view wx:if="{{item.carAuthDate}}" class='info_item'>
								<view class='info_intem_label'>授权日期:</view><label>{{item.carAuthDate}}</label><label>(车)</label>
							</view>
							<view wx:if="{{item.objectTypeName}}" class='info_item'>
								<view class='info_intem_label'>物品类型:</view><label>{{item.objectTypeName}}</label>
							</view>
							<view wx:if="{{item.objectCount}}" class='info_item'>
								<view class='info_intem_label'>数量:</view><label>{{item.objectCount}}</label>
							</view>
							<view wx:if="{{item.objectStartDate}}" class='info_item'>
								<view class='info_intem_label'>授权日期:</view><label>{{item.objectStartDate}}</label><label>起(物)</label>
								<view class='info_intem_label'></view><label>{{item.objectEndDate}}</label><label>止(物)</label>
							</view>
							<view wx:if="{{item.submitTime}}" class='info_item'>
								<view class='info_intem_label'>提交时间:</view><label>{{item.submitTime}}</label>
							</view>
							<view class='info_item' wx:if="{{item.rejectReason}}">
								<view class='info_intem_label'>驳回原因:</view><label>{{item.rejectReason}}</label>
							</view>
						</view>
						<image wx:if="{{item.statusName=='审核中'}}" src="{{shzimg}}" class='item_info_right'> </image>
						<image wx:if="{{item.statusName=='审批通过'}}" src="{{sptgimg}}" class='item_info_right'> </image>
						<image wx:if="{{item.statusName=='审批中'}}" src="{{spzimg}}" class='item_info_right'> </image>
						<image wx:if="{{item.statusName=='已过期'}}" src="{{ygqimg}}" class='item_info_right'> </image>
						<image wx:if="{{item.statusName=='已驳回'}}" src="{{spbhimg}}" class='item_info_right'> </image>
						<view style='clear:left;'></view>
					</view>
				</view>
			</view>
			<!-- 车辆入厂申清 -->
			<view class='item_content' wx:if="{{item.busType==1}}">
				<view class='item_content_title item_content_title_car'>
					<label class='item_content_title_left'>车辆入厂申请</label><button wx:if="{{item.statusName=='审批通过'}}"
						class="cu-btn round line-white item_content_right_button" data-bustype="{{item.busType}}"
						data-objuid="{{item.objuid}}" bindtap="qrcode">二维码</button>
				</view>
				<view style='position: relative;'>
					<view class='item_info'>
						<view class='item_info_left'>
							<view class='info_item'>
								<view class='info_intem_label'>驾驶人:</view><label>{{item.name}}</label>
							</view>
							<view class='info_item'>
								<view class='info_intem_label'>手机号:</view><label>{{item.phoneNumber}}</label>
							</view>
							<view class='info_item'>
								<view class='info_intem_label'>车牌号:</view><label>{{item.plateNumber}}</label>
							</view>
							<view class='info_item'>
								<view class='info_intem_label'>车类型:</view><label>{{item.carTypeName}}</label>
							</view>
							<view class='info_item'>
								<view class='info_intem_label'>接待人:</view><label>{{item.receptionist}}</label>
							</view>
							<view class='info_item'>
								<view class='info_intem_label'>授权日期:</view><label>{{item.carAuthDate}}</label>
							</view>
							<view class='info_item'>
								<view class='info_intem_label'>提交时间:</view><label>{{item.submitTime}}</label>
							</view>
							<view class='info_item' wx:if="{{item.rejectReason}}">
								<view class='info_intem_label'>驳回原因:</view><label>{{item.rejectReason}}</label>
							</view>
						</view>
						<image wx:if="{{item.statusName=='审核中'}}" src="{{shzimg}}" class='item_info_right'> </image>
						<image wx:if="{{item.statusName=='审批通过'}}" src="{{sptgimg}}" class='item_info_right'> </image>
						<image wx:if="{{item.statusName=='审批中'}}" src="{{spzimg}}" class='item_info_right'> </image>
						<image wx:if="{{item.statusName=='已过期'}}" src="{{ygqimg}}" class='item_info_right'> </image>
						<image wx:if="{{item.statusName=='已驳回'}}" src="{{spbhimg}}" class='item_info_right'> </image>
						<view style='clear:left;'></view>
					</view>
				</view>
			</view>
			<!-- 物品入厂申清 -->
			<view class='item_content' wx:if="{{item.busType==2}}">
				<view class='item_content_title item_content_title_goods'>
					<label class='item_content_title_left'>(限制)物品申请</label><button wx:if="{{item.statusName=='审批通过'}}"
						class="cu-btn round line-white item_content_right_button" data-bustype="{{item.busType}}"
						data-objuid="{{item.objuid}}" bindtap="qrcode">二维码</button>
				</view>
				<view style='position: relative;'>
					<view class='item_info'>
						<view class='item_info_left'>
							<view class='info_item'>
								<view class='info_intem_label'>携带人:</view><label>{{item.name}}</label>
							</view>
							<view class='info_item'>
								<view class='info_intem_label'>手机号:</view><label>{{item.phoneNumber}}</label>
							</view>
							<view class='info_item'>
								<view class='info_intem_label'>物品类型:</view><label>{{item.objectTypeName}}</label>
							</view>
							<view class='info_item'>
								<view class='info_intem_label'>数量:</view><label>{{item.objectCount}}</label>
							</view>
							<view class='info_item'>
								<view class='info_intem_label'>接待人:</view><label>{{item.receptionist}}</label>
							</view>
							<view class='info_item'>
								<view class='info_intem_label'>授权日期:</view><label>{{item.objectStartDate}}</label><label>起</label>
								<view class='info_intem_label'></view><label>{{item.objectEndDate}}</label><label>止</label>
							</view>
							<view class='info_item'>
								<view class='info_intem_label'>提交时间:</view><label>{{item.submitTime}}</label>
							</view>
							<view class='info_item' wx:if="{{item.rejectReason}}">
								<view class='info_intem_label'>驳回原因:</view><label>{{item.rejectReason}}</label>
							</view>
						</view>
						<image wx:if="{{item.statusName=='审核中'}}" src="{{shzimg}}" class='item_info_right'> </image>
						<image wx:if="{{item.statusName=='审批通过'}}" src="{{sptgimg}}" class='item_info_right'> </image>
						<image wx:if="{{item.statusName=='审批中'}}" src="{{spzimg}}" class='item_info_right'> </image>
						<image wx:if="{{item.statusName=='已过期'}}" src="{{ygqimg}}" class='item_info_right'> </image>
						<image wx:if="{{item.statusName=='已驳回'}}" src="{{spbhimg}}" class='item_info_right'> </image>
						<view style='clear:left;'></view>
					</view>
				</view>
			</view>
		</view>
		<view wx:if="{{lastpage}}" class="cu-load bg-blue2 over"></view>
		<view wx:else>
			<view wx:if="{{loading}}" class="cu-load bg-blue2 loading"></view>
		</view>
	</view>
	<!-- 列表元素结束 -->
</view>

<!-- 驳回原因窗口 -->
<view class="cu-modal {{modalNameBH=='DialogModal'?'show':''}}">
	<view class="cu-dialog" style="margin-top: -200px;">
		<view class="cu-bar bg-blue2 justify-end">
			<view class="content">驳回原因</view>
		</view>
		<view style="height:80px;">
			<input type="text" bindinput="reason" value="{{reason}}" style="font-size:16px;height:40px;text-align:left;padding-left:10px;margin-top: 38px;"></input>
			<!-- <textarea style="text-align:left;height:90px;margin-top:10px;font-size:16px;width:100%;padding: 10px;" bindinput="reason"></textarea> -->
		</view>
		<view class="cu-bar bg-white justify-end">
			<view class="action">
				<button class="cu-btn line-blue text-blue" bindtap="rejectReasonHide">取消</button>
				<button class="cu-btn bg-blue2 margin-left" bindtap="reject">确认驳回</button>
			</view>
		</view>
	</view>
</view>